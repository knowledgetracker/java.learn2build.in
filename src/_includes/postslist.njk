<table class="table table-bordered postlist">
<thead>
<tr><th>Sno</th><th>Title</th><th>Duration</th><th>Solved</th> <th>Practice</th><th>HOTS</th><th>Planned Date</th><th>Status</th><th>Action</th></tr>
</thead>
<tbody>

{% for post in postslist  %}
  {% if post.data.published != false %}
  <tr>
  <td>{{ loop.index }}</td>
  <td >
    <a href="{{ post.url | url }}" target="_blank" >
    {% if post.data.title %}{{ post.data.title }}{% else %}<code>{{ post.url }}</code>{% endif %}</a>
  </td>
  <td>{{post.data.duration}}</td>
    <td>{{post.data.solved}}</td>
    <td>{{post.data.practice}}</td>
    <td>{{post.data.hots}}</td>
    
  <td>
  <time class="postlist-date" datetime="{{ post.date | htmlDateString }}">{{ post.date | readableDate }}</time>
  </td>
  <td>{{post.data.published}}</td>
  <td>
   <a href="{{ post.url | url }}"  class="btn btn-info btn-sm text-white">View</a>
  </td>
  </tr>
  {% endif %}
{% endfor %}
</tbody>
</table>

